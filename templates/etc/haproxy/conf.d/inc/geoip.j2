    http-request set-map({{ HAPROXY_HC.path.map_geoip_asn }}) %[src,ipmask(24,48)] %[var(txn.geoip_asn)] if !private_nets !geoip_asn_in_map
    http-request capture var(txn.geoip_asn) len 10
{% endif %}

